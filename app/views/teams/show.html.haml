- content_for(:title) do
  = "#{@team.name} - Launch Academy"

%h2= "Team: #{@team.name}"

%h3 Members

%ul.block#members
  - @team.users.each do |user|
    %li= link_to(user.username, user)

%h3 Assignments

%ul.block#assignments
  - @team.assignments.each do |assignment|
    %li= link_to(assignment.lesson.title, assignment.lesson)
